<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.third.autoloan.ordermag.mapper.OrderMapper">
 	
 	<!-- 得到一共有多少条数据 -->
 	<select id="getTotalOrderNumByMap" resultType="int">
 		select count(id) from t_order as o 
 		 <where>  
            <if test="params.contractNumber!=null">  
          		AND o.contractNumber like concat(#{params.contractNumber},'%')
            </if>  
        </where>  
 	</select>
 </mapper>
